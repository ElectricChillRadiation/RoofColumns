<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <RoofDef>
    <defName>RoofColumnInstanceConstructedRoof</defName>
    <label>artificial roof</label>
    <isThickRoof>false</isThickRoof>
    <filthLeaving>Filth_RubbleBuilding</filthLeaving>
    <soundPunchThrough>PunchThroughRoofMetal</soundPunchThrough>
  </RoofDef>

  <RoofDef>
    <defName>RoofColumnInstanceThinRoof</defName>
    <label>artificial natural-like roof</label>
    <isThickRoof>false</isThickRoof>
    <isNatural>true</isNatural>
    <filthLeaving>Filth_RubbleBuilding</filthLeaving>
    <soundPunchThrough>PunchThroughRoofMetal</soundPunchThrough>
  </RoofDef>

  <RoofDef>
    <defName>RoofColumnInstanceMountainRoof</defName>
    <label>artificial impenetrable roof</label>
    <isThickRoof>true</isThickRoof>
    <isNatural>true</isNatural>
    <collapseLeavingThingDef>CollapsedRocks</collapseLeavingThingDef>
    <filthLeaving>Filth_RubbleBuilding</filthLeaving>
    <soundPunchThrough>PunchThroughRoofMetal</soundPunchThrough>
  </RoofDef>

</Defs>
